             mc:Ignorable="d"
			 d:DesignHeight="340" d:DesignWidth="600">
	
	<Grid Background="#FFE5E5E5">
		<DataGrid Name="usersDataGrid" Margin="10,10,10,10" CanUserAddRows="False" AutoGeneratingColumn="usersDataGrid_AutoGeneratingColumn" Loaded="usersDataGrid_Loaded"/>
	</Grid>
</UserControl>
